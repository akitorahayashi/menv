{
  "general": {
    "preferredEditor": "code",
    "vimMode": false,
    "checkpointing": {
      "enabled": false
    },
    "previewFeatures": true
  },
  "ui": {
    "theme": "GitHub",
    "hideBanner": false,
    "hideTips": false,
    "showMemoryUsage": true,
    "showLineNumbers": false,
    "showCitations": true
  },
  "ide": {
    "enabled": false,
    "hasSeenNudge": true
  },
  "privacy": {
    "usageStatisticsEnabled": false
  },
  "model": {
    "name": "gemini-3-pro-preview"
  },
  "context": {
    "fileName": [
      "GEMINI.md",
      "AGENTS.md"
    ],
    "includeDirectories": [],
    "loadFromIncludeDirectories": true,
    "fileFiltering": {
      "respectGitIgnore": true,
      "respectGeminiIgnore": true,
      "enableRecursiveFileSearch": true
    }
  },
  "tools": {
    "sandbox": false,
    "exclude": []
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "advanced": {
    "excludedEnvVars": []
  },
  "telemetry": {
    "enabled": false,
    "target": "local",
    "outfile": ".gemini/telemetry.log"
  },
  "output": {
    "format": "text"
  }
}